{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.py"
    },
    {
      "src": "/api/credits",
      "dest": "/api/credits.py"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}
